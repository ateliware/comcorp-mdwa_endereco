<%% if @estado.errors.any? %>
	$("#mdwa_error").html("<%%= j( render '/template/mdwa/crud_error', :object => @estado )%>");
<%% else %>
	$.fancybox.close(true);
	$('#estados_filter_form').submit();
	$('body').append("<%%= j( render '/template/mdwa/notice', :notice => @system_notice )%>");
<%% end %>